# Import your predictor
from hybrid_cat_predictor import train_model, predict_activity
import pandas as pd

# Load your dataset (replace with your real descriptors)
df_features = pd.read_csv("data/processed/example_features.csv")

# Train model
X = df_features[["bandgap_eV", "CBM_eV", "VBM_eV", "POM_LUMO_eV", "CO2_adsorption_eV", "stability_index"]]
y = df_features["activity_score"]
model = train_model(X, y)

# Predict new candidates
predictions = predict_activity(model, X)
